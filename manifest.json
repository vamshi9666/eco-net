 {
    "manifest_version": 2,
    "name": "eco-mon",
    "version": "1.2",
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["index.js"]
      }
    ],
    "background": {
      "scripts": ["input.js"]
    },

    "permissions": ["tabs","theme"]
}
